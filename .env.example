# Environment configuration example for the agent

# Flask configuration
FLASK_ENV=development  # Options: development, production
FLASK_DEBUG=1          # Options: 0 (off), 1 (on)

# Agent configuration
ASSISTANT_ID=your_default_assistant_id_here

# Database configuration (if applicable)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=your_database_name
DB_USER=your_database_user
DB_PASSWORD=your_database_password

# API keys and secrets
API_KEY=your_api_key_here
SECRET_KEY=your_secret_key_here

# NGINX Config
DOMAIN=DOMAIN
NGINX_SUBDOMAIN=NGINX_SUBDOMAIN
NGINX_PORT=NGINX_PORT
WAZUH_SUBDOMAIN=WAZUH_SUBDOMAIN
WAZUH_PORT=WAZUH_PORT
GITLAB_SUBDOMAIN=GITLAB_SUBDOMAIN
GITLAB_PORT=GITLAB_PORT
PORTAINER_SUBDOMAIN=PORTAINER_SUBDOMAIN
PORTAINER_PORT=PORTAINER_PORT
N8N_SUBDOMAIN=N8N_SUBDOMAIN
N8N_PORT=N8N_PORT

# Zerotier Config
ZEROTIER_SUBNET=ZEROTIER_SUBNET
ZEROTIER_NETWORK_ID=ZEROTIER_NETWORK_ID
ZEROTIER_NETWORK_IP=ZEROTIER_NETWORK_IP

# Github Config
GITHUB_USER=GITHUB_USER
GITHUB_PASS=GITHUB_PASS
GITHUB_EMAIL=GITHUB_EMAIL
GITHUB_TOKEN=GITHUB_TOKEN

# Slack Config
SLACK_CLIENT_ID=SLACK_CLIENT_ID
SLACK_CLIENT_SECRET=SLACK_CLIENT_SECRET

# Discord Config
DISCORD_TOKEN=DISCORD_TOKEN
DISCORD_CLIENT_ID=DISCORD_CLIENT_ID

# N8N Config
N8N_DOMAIN=N8N_DOMAIN
N8N_USER=N8N_USER
N8N_PASS=N8N_PASS
N8N_EMAIL=N8N_EMAIL

# Logging configuration
LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARNING, ERROR, CRITICAL

# CORS configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://yourdomain.com

# External service URLs
EXTERNAL_SERVICE_URL=https://api.external-service.com

# Update instructions for Docker environment
# Dockerfile updates
DFILE_UPDATE_1=DFILE_UPDATE_1
DFILE_UPDATE_2=DFILE_UPDATE_2

# docker-compose.yml updates
DCFILE_UPDATE_1=DCFILE_UPDATE_1
DCFILE_UPDATE_2=DCFILE_UPDATE_2

# .env updates
ENV_UPDATE_1=ENV_UPDATE_1
ENV_UPDATE_2=ENV_UPDATE_2

# package.json updates
PKG_UPDATE_1=PKG_UPDATE_1
PKG_UPDATE_2=PKG_UPDATE_2

# Other configurations
# Add any other environment variables your application might need